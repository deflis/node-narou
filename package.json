{
  "name": "narou",
  "version": "0.9.3",
  "description": "Narou API Wrapper",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "browser": {
        "require": "./dist/index.browser.cjs",
        "import": "./dist/index.browser.js",
        "types": "./dist/index.browser.d.ts"
      }
    },
    "./browser": {
      "default": "./dist/index.browser.js",
      "require": "./dist/index.browser.cjs",
      "import": "./dist/index.browser.js",
      "types": "./dist/index.browser.d.ts"
    },
    "./*": {
      "default": "./dist/*.js",
      "require": "./dist/*.cjs",
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./utils": {
      "default": "./dist/utils/*.js",
      "require": "./dist/utils/*.cjs",
      "import": "./dist/utils/*.js",
      "types": "./dist/utils/*.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "deflis <deflis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deflis/node-narou.git"
  },
  "dependencies": {
    "date-fns": "^3.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "scripts": {
    "build": "npm-run-all check build:tsc",
    "build:tsc": "tsup ./src",
    "check": "npm-run-all --parallel check:lint check:build test",
    "check:lint": "eslint src/** test/**",
    "check:build": "tsc --noEmit",
    "prepack": "npm-run-all format build",
    "format": "eslint --fix src/** test/**",
    "test": "vitest run",
    "docs": "npm-run-all docs:clean docs:typedoc",
    "docs:clean": "npx rimraf docs",
    "docs:typedoc": "npx typedoc --out ./docs src/index.ts"
  }
}
