{
  "name": "narou",
  "version": "0.2.0",
  "description": "Narou API Wrapper",
  "main": "src/index",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "yarn.lock"
  ],
  "author": "deflis <deflis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deflis/node-narou.git"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "moment": "^2.20.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "@types/power-assert": "^1.4.29",
    "axios-mock-adapter": "^1.12.0",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "mocha-typescript": "^1.1.12",
    "nyc": "^11.4.1",
    "postinstall-build": "^5.0.1",
    "power-assert": "^1.4.4",
    "remap-istanbul": "^0.10.1",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.1",
    "source-map-support": "^0.5.3",
    "touch": "^3.1.0",
    "ts-node": "^4.1.0",
    "typedoc": "^0.10.0",
    "typemoq": "^2.1.0",
    "typescript": "^2.7.1"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -d",
    "pretest": "npm run build",
    "test": "mocha --recursive --require ts-node/register \"test/**/*.ts\"",
    "precover": "npm run clean",
    "cover": "nyc npm test",
    "clean": "rimraf src/**/*.js src/**/*.js.map src/**/*.d.ts",
    "docs": "typedoc --out ./docs src/index.ts",
    "postdocs": "nodetouch ./docs/.nojekyll"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text"
    ],
    "sourceMap": true
  }
}
