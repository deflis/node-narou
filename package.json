{
  "name": "narou",
  "version": "0.9.2",
  "description": "Narou API Wrapper",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "browser": {
        "require": "./dist/index.browser.cjs",
        "import": "./dist/index.browser.js",
        "types": "./dist/index.browser.d.ts"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "author": "deflis <deflis@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/deflis/node-narou.git"
  },
  "dependencies": {
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "optionalDependencies": {
    "node-fetch": "^2.6.7"
  },
  "scripts": {
    "build": "npm-run-all check build:tsc",
    "build:tsc": "tsup ./src",
    "check": "npm-run-all --parallel check:lint check:build test",
    "check:lint": "eslint src/** test/**",
    "check:build": "tsc --noEmit",
    "prepack": "npm-run-all format build",
    "format": "eslint --fix src/** test/**",
    "test": "jest",
    "docs": "npm-run-all docs:clean docs:typedoc",
    "docs:clean": "npx rimraf docs",
    "docs:typedoc": "npx typedoc --out ./docs src/index.ts"
  }
}
